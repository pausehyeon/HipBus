<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Main">
	<insert id="insertMember" parameterType="model.MemberDto">
		insert into HB_Member
		values(
		#{email}, #{nick}, #{passwd}, 0,
		#{joindate}, #{imgname}, #{imglocation}
		)
	</insert>

	<select id="getMember" parameterType="String" resultType="model.MemberDto">
		select * from HB_Member where email=#{email}
	</select>

</mapper>